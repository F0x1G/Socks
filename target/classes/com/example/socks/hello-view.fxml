    <Button fx:id="Start" text="Почати підбір" onAction="#onStartClick"/>
    <TextField fx:id="LabelM" text="Висота"/>
    <TextField fx:id="LabelN" text="Висота"/>
    <Button fx:id="Resithe13" text="збільшити на 30%" onAction="#onRes30Click"/>
